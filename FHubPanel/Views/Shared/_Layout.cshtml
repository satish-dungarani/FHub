        function ChnageRequestStatus(_Id, _Status, _CallBackStatus) {
            //
            $.ajax({
                type: 'get',
                dataType: 'html',
                cache: false,
                url: '@Url.Action("ChangeRequestStatus", "Base")',
                data: { _Id: _Id, _Status: _Status },
                beforeSend: function () {
                    $("#ajaxLoader").show();
                },
                success: function (response) {
                    $("#ajaxLoader").hide();
                    if (_CallBackStatus == 'Top') {
                        if (response != null) {
                            $("#RequestList").html('');
                            $("#RequestList").html(response);
                            $("#RequestList").addClass('open');
                        }
                    }
                    else if (_CallBackStatus == 'Contact') {
                        //
                        if ($("#hidVendorStatus").val() == "All") {
                            $("#ContactContainer").empty();
                            pageSize = 15;
                            pageIndex = 0;
                            GetData();
                        }
                        else
                            $("#div" + _Id).remove();
                    }
                },
                error: function (ex, st, th) {
                    $("#ajaxLoader").hide();
                    toastr["error"](ex.responseText.substring(ex.responseText.indexOf('<title>') + 7, ex.responseText.indexOf('</title>')));
                }

            })
        }

